    "keywords": [
        "modestnerd",
        "localregex"
    ],
    "homepage": "https://github.com/modestnerd/localregex",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ngonidzashe Mangudya",
            "email": "iamngoni@modestnerd.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
